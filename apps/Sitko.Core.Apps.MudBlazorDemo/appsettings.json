{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    }
  },
  "AllowedHosts": "*",
  "Storage": {
    "FileSystem": {
      "TestBlazorStorageOptions": {
        "PublicUri": "https://localhost:5001/static/",
        "Name": "Test",
        "StoragePath": "wwwroot/static"
      }
    },
    "Metadata": {
      "Postgres": {
        "TestBlazorStorageOptions": {
          "Database": "dbname"
        }
      }
    },
    "Remote": {
      "TestRemoteStorageOptions": {
        "PublicUri": "https://localhost:5001/static/",
        "Name": "Remote",
        "RemoteUrl": "https://localhost:5001/Upload"
      }
    }
  },
  "MudLayout": {
    "Theme": "Dark"
  },
  "Application": {
    "Name": "MudBlazorDemo",
    "Version": "8.0.0"
  }
}
